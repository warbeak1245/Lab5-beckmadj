import static org.junit.Assert.*;

import java.util.ArrayList;

import org.junit.Test;



public class TestPrimeGeneration {
	@Test
	public void testTwo(){
		assertEquals(new ArrayList<Integer>(), PrimeGeneration.generatePrimes(2));
	}
	
	@Test
	public void testThree(){
		ArrayList<Integer> stuff = new ArrayList<Integer>();
		stuff.add(2);
		assertEquals(stuff, PrimeGeneration.generatePrimes(3));
	}
	
	@Test
	public void testThriteen(){
		ArrayList<Integer> stuff = new ArrayList<Integer>();
		stuff.add(2);
		stuff.add(3);
		stuff.add(5);
		stuff.add(7);
		stuff.add(11);
		assertEquals(stuff, PrimeGeneration.generatePrimes(13));
	}
	
	@Test
	public void testEightySeven(){
		ArrayList<Integer> stuff = new ArrayList<Integer>();
		stuff.add(2);
		stuff.add(3);
		stuff.add(5);
		stuff.add(7);
		stuff.add(11);
		stuff.add(13);
		stuff.add(17);
		stuff.add(19);
		stuff.add(23);
		stuff.add(29);
		stuff.add(31);
		stuff.add(37);
		stuff.add(41);
		stuff.add(43);
		stuff.add(47);
		stuff.add(53);
		stuff.add(59);
		stuff.add(61);
		stuff.add(67);
		stuff.add(71);
		stuff.add(73);
		stuff.add(79);
		stuff.add(83);
		assertEquals(stuff, PrimeGeneration.generatePrimes(87));
	}
	
	@Test
	public void testEightyEight(){
		ArrayList<Integer> stuff = new ArrayList<Integer>();
		stuff.add(2);
		stuff.add(3);
		stuff.add(5);
		stuff.add(7);
		stuff.add(11);
		stuff.add(13);
		stuff.add(17);
		stuff.add(19);
		stuff.add(23);
		stuff.add(29);
		stuff.add(31);
		stuff.add(37);
		stuff.add(41);
		stuff.add(43);
		stuff.add(47);
		stuff.add(53);
		stuff.add(59);
		stuff.add(61);
		stuff.add(67);
		stuff.add(71);
		stuff.add(73);
		stuff.add(79);
		stuff.add(83);
		assertEquals(stuff, PrimeGeneration.generatePrimes(88));
	}
	
	
}
