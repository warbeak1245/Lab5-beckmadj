import static org.junit.Assert.*;

import java.util.ArrayList;

import org.junit.Test;



public class TestPrimeGeneration {
	@Test
	public void testTwo(){
		assertEquals(new ArrayList<Integer>(), PrimeGeneration.generatePrimes(2));
	}
	
	@Test
	public void testThree(){
		ArrayList<Integer> stuff = new ArrayList<Integer>();
		stuff.add(2);
		assertEquals(stuff, PrimeGeneration.generatePrimes(3));
	}
	
	public void testThriteen(){
		ArrayList<Integer> stuff = new ArrayList<Integer>();
		stuff.add(2);
		stuff.add(3);
		stuff.add(5);
		stuff.add(7);
		stuff.add(11);
		assertEquals(stuff, PrimeGeneration.generatePrimes(13));
	}
}
